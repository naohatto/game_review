<main>
  <div class="container">
    <div class="row">
      <div class="col">
      <%= form_with model: @post do |f| %>
        <h4>Title</h4>
        <%= f.text_field :title %>
        <h4>Review</h4>
        <%= f.text_area :body %>
        <h4>Rate</h4>
        <%#= f.text_field :rate %>
        <%= f.submit 'Create Review' %>
      <% end %>
      </div>
    </div>
  </div>
</main>